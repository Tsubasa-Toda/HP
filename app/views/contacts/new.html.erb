<h1>Contact Us</h1>
<%= form_for @contact, url: contacts_path do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :message %>
  <%= f.text_area :message %>
  <%= f.submit "Submit" %>
<% end %>
<%= render "shared/footer" %>
